<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cc.admin.dao.HouseMapper">
    
    <select id="query"  resultType="com.cc.admin.dto.House">
    	SELECT 
    		*
        	FROM 
    		House
    			
    </select>

    
       
   

    <update id="update" parameterType="com.cc.admin.dto.Port">  
       update House set used=#{used},remained=#{remained} where id=#{id}  
    </update>  
    
  
</mapper>